{
  "name": "detoxify",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/dev -e js,twig --exec babel-node --presets es2015,env",
    "transpile": "babel --presets es2015,env -d ./build ./src -s",
    "copy-assets": "cp -r ./src/views ./build && cp -r ./src/public ./build",
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && npm run transpile && npm run copy-assets"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "google-play-scraper": "^6.2.6",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "twig": "~0.10.3",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "nodemon": "^1.19.1"
  }
}
